<h1>新規登録</h1>
<% @user.errors.full_messages.each do |message| %>
    <%= message %>
<% end %>
<%= form_tag("/users/create")do %>
    <p>ユーザー名</p>
    <input name="name">
    <p>メールアドレス</p>
    <input name="email">
    <p>パスワード</p>
    <input name="password" type="password" value="<%= @user.password %>">
    <input type="submit" value="新規登録">
<% end %>